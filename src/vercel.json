{
  "redirects": [
    { "source": "/", "destination": "home-page.html", "permanent": true }
  ],
  "rewrites": [],
  "headers": [
    {
      "source": "/(.*).(css|js)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/images/:path*",
      "destination": "/public/images/:path*"
    }
  ]
}
