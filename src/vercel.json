{
  "redirects": [
    {
      "source": "/",
      "destination": "/home-page.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
